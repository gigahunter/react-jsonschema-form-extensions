{
    "name": "react-jsonschema-form-extensions",
    "version": "1.0.7",
    "description": "extensions for react-jsonschema-form, and forked from Narazaka/react-jsonschema-form-layout-grid",
    "main": "lib/index.js",
    "module": "es/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "prepare": "npm run build",
        "build": "nwb build-react-component",
        "clean": "nwb clean-module && npm clean-demo",
        "start": "nwb serve-react-demo",
        "test": "jest --coverage",
        "test:coverage": "jest --coverage",
        "test:watch": "nwb test-react --server",
        "demo": "nwb build-demo"
    },
    "dependencies": {
        "antd": "^3.26.14",
        "react-bootstrap": "^0.33.1",
        "react-jsonschema-form": "^1.8.1",
        "react-rte": "^0.16.1",
        "react-tag-autocomplete2": "^1.0.1"
    },
    "peerDependencies": {
        "react": "16.x"
    },
    "devDependencies": {
        "babel-eslint": "7.1.1",
        "babel-jest": "^19.0.0",
        "babel-preset-es2015": "6.22.0",
        "babel-preset-react": "6.23.0",
        "babel-preset-stage-0": "6.22.0",
        "coveralls": "^3.0.11",
        "eslint": "3.16.1",
        "eslint-plugin-react": "6.10.0",
        "jest": "19.0.2",
        "nwb": "^0.23.0",
        "nwb-less": "^0.7.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-dropzone": "^4.2.3",
        "react-test-renderer": "^16.13.1"
    },
    "author": "gigahunter",
    "homepage": "",
    "license": "MIT",
    "repository": "https://github.com/gigahunter/react-jsonschema-form-extensions",
    "keywords": [
        "react-component"
    ],
    "babel": {
        "presets": [
            "es2015",
            "stage-0",
            "react"
        ]
    },
    "jest": {
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        }
    }
}
